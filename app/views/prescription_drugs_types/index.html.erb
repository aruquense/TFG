<p id="notice"><%= notice %></p>

<h1>Prescription Drugs Types</h1>

<table>
  <thead>
    <tr>
      <th>Idn</th>
      <th>Description</th>
      <th>Description en</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prescription_drugs_types.each do |prescription_drugs_type| %>
      <tr>
        <td><%= prescription_drugs_type.idn %></td>
        <td><%= prescription_drugs_type.description %></td>
        <td><%= prescription_drugs_type.description_EN %></td>
        <td><%= link_to 'Show', prescription_drugs_type %></td>
        <td><%= link_to 'Edit', edit_prescription_drugs_type_path(prescription_drugs_type) %></td>
        <td><%= link_to 'Destroy', prescription_drugs_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prescription Drugs Type', new_prescription_drugs_type_path %>
