<h2>Resumen de Tests Realizados</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Idn</th>
      <th>Exploration type</th>
      <th>Test</th>
      <th>SCORE</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.idn %></td>
        <td><%= ExplorationType.find(test.exploration_type_id).description %></td>
        <td><%= Test.find(test.test_id).description %></td>
        <td><%= test.score %></td>
        <td><%= link_to 'Show', test %></td>
        <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
