<p id="notice"><%= notice %></p>

<h1>Patient Aacc Diseases</h1>

<table>
  <thead>
    <tr>
      <th>Idn</th>
      <th>Description</th>
      <th>Description en</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_aacc_diseases.each do |patient_aacc_disease| %>
      <tr>
        <td><%= patient_aacc_disease.idn %></td>
        <td><%= patient_aacc_disease.description %></td>
        <td><%= patient_aacc_disease.description_en %></td>
        <td><%= link_to 'Show', patient_aacc_disease %></td>
        <td><%= link_to 'Edit', edit_patient_aacc_disease_path(patient_aacc_disease) %></td>
        <td><%= link_to 'Destroy', patient_aacc_disease, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Aacc Disease', new_patient_aacc_disease_path %>
